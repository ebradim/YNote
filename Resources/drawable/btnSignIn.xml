<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >

  <item android:state_pressed="true" >
    <shape android:shape="rectangle"  >
      <corners android:radius="35dip" />
      <gradient android:angle="45" android:startColor="#34fa7f"      android:endColor="#0dbfb1"  />
    </shape>
  </item>
  <item android:state_focused="true">
    <shape android:shape="rectangle"  >
      <corners android:radius="35dip" />
      <solid android:color="#38ef7d"/>
    </shape>
  </item>
  <item >
    <shape android:shape="rectangle"  >
      <corners android:radius="35dip" />
      <gradient android:angle="45" android:startColor="#38ef7d" android:endColor="#11998e" />
    </shape>
  </item>
  
</selector>